{
  "name": "stagstation",
  "version": "1.0.0",
  "description": "Save file converter for Hollow Knight and Silksong between PC and Switch",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron . --dev\"",
    "start": "npm run build && electron ."
  },
  "keywords": [
    "hollow-knight",
    "silksong",
    "save-converter"
  ],
  "author": "",
